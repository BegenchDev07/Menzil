<%- include('header_begn_ru'); %>
<div class="container col-xxl-8 px-4 py-5">
    <div id="carouselExampleControls" class="carousel slide" data-ride="carousel">
        <% for(var i = 0; i < data.filename.length; i++){ %> 
        <div class="carousel-inner">
            <img class="d-block w-100" src="/products/<%=data.filename[i]%>" alt="First slide">
            <br>
          <%}%>
          <hr>
          <h2> <b> Имя:      </b> </h2> <p> <%=data.name%> </p>
          <h2> <b> Описание: </b> </h2> <p><%=data.description%></p>
          <h2> <b> Цена:    </b> </h2> <p><%=data.price%></p>
          <h2> <b> Количество:    </b> </h2> <p><%=data.pieces%></p>
        </div>
        <a class="btn btn-outline-primary" href="/ru/Products">Обратно</a>
    </div>
<%- include('header_end_ru'); %>